<?xml version="1.0"?>
<doc>
    <assembly>
        <name>Affirma.ThreeSharp.Wrapper</name>
    </assembly>
    <members>
        <member name="T:Affirma.ThreeSharp.Wrapper.ThreeSharpWrapper">
            <summary>
            Wraps basic functionality of the ThreeSharp Library into single-line commands
            </summary>
        </member>
        <member name="M:Affirma.ThreeSharp.Wrapper.ThreeSharpWrapper.AddBucket(System.String)">
            <summary>
            Adds a bucket to an S3 account
            </summary>
        </member>
        <member name="M:Affirma.ThreeSharp.Wrapper.ThreeSharpWrapper.ListBucket(System.String)">
            <summary>
            Returns a string of XML, describing the contents of a bucket
            </summary>
        </member>
        <member name="M:Affirma.ThreeSharp.Wrapper.ThreeSharpWrapper.AddStringObject(System.String,System.String,System.String)">
            <summary>
            Adds a string to a bucket, as an object
            </summary>
        </member>
        <member name="M:Affirma.ThreeSharp.Wrapper.ThreeSharpWrapper.AddFileObject(System.String,System.String,System.String)">
            <summary>
            Streams a file to a bucket as an object
            </summary>
        </member>
        <member name="M:Affirma.ThreeSharp.Wrapper.ThreeSharpWrapper.AddEncryptFileObject(System.String,System.String,System.String,System.String,System.String)">
            <summary>
            Streams a file to a bucket as an object, with encryption
            </summary>
        </member>
        <member name="M:Affirma.ThreeSharp.Wrapper.ThreeSharpWrapper.GetStringObject(System.String,System.String)">
            <summary>
            Gets a string object from a bucket, and returns it as a String
            </summary>
        </member>
        <member name="M:Affirma.ThreeSharp.Wrapper.ThreeSharpWrapper.GetFileObject(System.String,System.String,System.String)">
            <summary>
            Gets a file object from a bucket, and streams it to disk
            </summary>
        </member>
        <member name="M:Affirma.ThreeSharp.Wrapper.ThreeSharpWrapper.GetDecryptFileObject(System.String,System.String,System.String,System.String,System.String)">
            <summary>
            Gets a file object from a bucket, streaming it to disk, with decryption
            </summary>
        </member>
        <member name="M:Affirma.ThreeSharp.Wrapper.ThreeSharpWrapper.CopyObject(System.String,System.String,System.String,System.String)">
            <summary>
            Copies an object from a source location to a destination location
            </summary>
        </member>
        <member name="M:Affirma.ThreeSharp.Wrapper.ThreeSharpWrapper.GetUrl(System.String,System.String)">
            <summary>
            Generates a URL to access an S3 object in a bucket
            </summary>
        </member>
        <member name="M:Affirma.ThreeSharp.Wrapper.ThreeSharpWrapper.DeleteObject(System.String,System.String)">
            <summary>
            Deletes an object from a bucket
            </summary>
        </member>
        <member name="M:Affirma.ThreeSharp.Wrapper.ThreeSharpWrapper.DeleteBucket(System.String)">
            <summary>
            Deletes a bucket from an S3 account
            </summary>
        </member>
    </members>
